{
  "extends": "eslint:recommended",
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "amd": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "semi": [2, "always"],
    "curly": 2,
    "dot-notation": 2,
    "radix": 2,
    "wrap-iife": 2,
    "eqeqeq": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "no-alert": 2,
    "no-new-wrappers": 2,
    "no-native-reassign": 2,
    "no-extend-native": 2,
    "no-implicit-coercion": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-warning-comments": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-with": 2
  }
}